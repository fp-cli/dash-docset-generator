<h3>SUBCOMMANDS</h3>
<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>
    </thead>
    <tbody>
    {% for command in commands %}
        <tr>
            <td>
                <a class="dashAnchor"
                   name="//apple_ref/cpp/Command/{{ command.name | url_encode }}"
                   data-relative-href="{{ command.relativeHref }}">
                    <code>fp {{ command.name }}</code>
                </a>
            </td>
            <td>{{ command.description }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
