{
  "name": "fp-cli/dash-docset-generator",
  "description": "Generates Dash docsets for FP-CLI",
  "version": "1.2.0",
  "type": "project",
  "keywords": [
    "dash",
    "docset",
    "fp-cli",
    "finpress"
  ],
  "homepage": "https://github.com/fp-cli/dash-docset-generator",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrik Csak",
      "email": "p@trikcsak.com"
    }
  ],
  "require": {
    "php": "8.0 - 8.2",
    "ext-sqlite3": "*",
    "alchemy/zippy": "^1.0",
    "composer/semver": "^3.2",
    "erusev/parsedown": "^1.6",
    "guzzlehttp/guzzle": "^7.0",
    "http-interop/http-factory-guzzle": "^1.0",
    "knplabs/github-api": "^3.0",
    "symfony/filesystem": "^5.4",
    "symfony/finder": "^5.4",
    "twbs/bootstrap": "^5.1",
    "twig/twig": "^3.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "DashWpCli\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
